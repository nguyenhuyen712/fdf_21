<tr>
  <td><%= l order.created_at.in_time_zone(t "time.zone") %></td>
  <td><%= order.receiver_name %></td>
  <td><%= order.receiver_address %></td>
  <td><%= order.receiver_phone %></td>
  <td><%= l order.delivery_time %></td>
  <td><%= order.status %></td>
  <td><%= link_to t("users.show.detail"), user_order_path(@user, order),
    class: "btn btn-info" %></td>
</tr>
