<% provide :title, t(".title") %>
<h1><%= t ".title" %></h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for @order, url: admin_order_path(@order) do |f| %>
      <%= f.select :status, options_for_select(@statuses, @order.status),{},
        class: "form-control" %>
      <%= f.submit (t ".title"), class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
